                <ComboBoxItem x:Name="PersonsInHomeTransgender4" Content="Transgender"/>
                <ComboBoxItem x:Name="PersonsInHomeOther4" Content="Other"/>
            </ComboBox>
            <TextBox x:Name="textBoxPersonsInHomeDOB4" HorizontalAlignment="Left" Height="16" Margin="396,478,0,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeDob4}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBox x:Name="textBoxPersonsInHomeRelationship5" Height="16" Margin="108,499,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship5}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender5" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender5}"  Margin="268,499,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale5" Content="Female"/>
                <ComboBoxItem x:Name="PersonsInHomeMale5" Content="Male"/>
                <ComboBoxItem x:Name="PersonsInHomeTransgender5" Content="Transgender"/>
                <ComboBoxItem x:Name="PersonsInHomeOther5" Content="Other"/>
            </ComboBox>
            <TextBox x:Name="textBoxPersonsInHomeDOB5" HorizontalAlignment="Left" Height="16" Margin="396,499,0,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeDob5}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBox x:Name="textBoxPersonsInHomeRelationship6" Height="16" Margin="108,520,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship6}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender6" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender6}"  Margin="268,520,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale6" Content="Female"/>
                <ComboBoxItem x:Name="PersonsInHomeMale6" Content="Male"/>
                <ComboBoxItem x:Name="PersonsInHomeTransgender6" Content="Transgender"/>
