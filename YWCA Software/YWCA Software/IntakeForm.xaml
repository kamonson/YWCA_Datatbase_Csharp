            <TextBox x:Name="textBoxPersonsInHomeRelationship" Height="16" Margin="108,415,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender}"  Margin="268,415,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale" Content="Female"/>
                <ComboBoxItem x:Name="PersonsInHomeMale" Content="Male"/>
                <ComboBoxItem x:Name="PersonsInHomeTransgender" Content="Transgender"/>
                <ComboBoxItem x:Name="PersonsInHomeOther" Content="Other"/>
            </ComboBox>
            <TextBox x:Name="textBoxPersonsInHomeDOB" HorizontalAlignment="Left" Height="16" Margin="396,415,0,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeDob}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBox x:Name="textBoxPersonsInHomeRelationship2" Height="16" Margin="108,436,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship2}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender2" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender2}"  Margin="268,436,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale2" Content="Female"/>
                <ComboBoxItem x:Name="PersonsInHomeMale2" Content="Male"/>
                <ComboBoxItem x:Name="PersonsInHomeTransgender2" Content="Transgender"/>
                <ComboBoxItem x:Name="PersonsInHomeOther2" Content="Other"/>
            </ComboBox>
            <TextBox x:Name="textBoxPersonsInHomeDOB2" HorizontalAlignment="Left" Height="16" Margin="396,436,0,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeDob2}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBox x:Name="textBoxPersonsInHomeRelationship3" Height="16" Margin="108,457,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship3}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender3" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender3}"  Margin="268,457,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale3" Content="Female"/>
                <ComboBoxItem x:Name="PersonsInHomeMale3" Content="Male"/>
                <ComboBoxItem x:Name="PersonsInHomeTransgender3" Content="Transgender"/>
                <ComboBoxItem x:Name="PersonsInHomeOther3" Content="Other"/>
            </ComboBox>
            <TextBox x:Name="textBoxPersonsInHomeDOB3" HorizontalAlignment="Left" Height="16" Margin="396,457,0,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeDob3}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBox x:Name="textBoxPersonsInHomeRelationship4" Height="16" Margin="108,478,261,0" TextWrapping="Wrap" Text="{Binding PersonsInHomeRelationship4}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox x:Name="comboBoxPersonsInHomeGender4" HorizontalAlignment="Left" Text ="{Binding PersonsInHomeGender4}"  Margin="268,478,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.Column="1">
                <ComboBoxItem x:Name="PersonsInHomeFemale4" Content="Female"/>
