<Window x:Class="YWCA_Software.IntakeForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YWCA_Software"
        mc:Ignorable="d"
        Title="IntakeForm" Height="900" Width="900">
    <Grid>
        <TextBlock x:Name="textBlockStaff" HorizontalAlignment="Left" Margin="424,10,0,0" TextWrapping="Wrap" Text="Staff:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxStaff" Height="16" Margin="455,10,357,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockPid" HorizontalAlignment="Left" Margin="131,30,0,0" TextWrapping="Wrap" Text="{Binding Pid}" Width="70" VerticalAlignment="Top"/>
        <TextBox x:Name="textBlockPidLabel" HorizontalAlignment="Left" Height="16" Margin="40,30,0,0" TextWrapping="Wrap" Text="Participant ID:" VerticalAlignment="Top" Width="86" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockFNameLabel" HorizontalAlignment="Left" Margin="206,30,0,0" TextWrapping="Wrap" Text="First Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxFName" HorizontalAlignment="Left" Height="16" Margin="271,30,0,0" TextWrapping="Wrap" Text="{Binding FirstName}" VerticalAlignment="Top" Width="80" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockMI" HorizontalAlignment="Left" Margin="356,30,0,0" TextWrapping="Wrap" Text="Middle Initial:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxMI" HorizontalAlignment="Left" Height="16" Margin="433,30,0,0" TextWrapping="Wrap" Text="{Binding Mi}" VerticalAlignment="Top" Width="25" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockLNameLabel" HorizontalAlignment="Left" Margin="463,30,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLName" HorizontalAlignment="Left" Height="16" Margin="526,30,0,0" TextWrapping="Wrap" Text="{Binding LastName}" VerticalAlignment="Top" Width="132" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockDOB" HorizontalAlignment="Left" Margin="663,30,0,0" TextWrapping="Wrap" Text="DOB:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxDOB" HorizontalAlignment="Left" Height="16" Margin="695,30,0,0" TextWrapping="Wrap" Text="{Binding Dob}" VerticalAlignment="Top" Width="107" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockHomeResidence" HorizontalAlignment="Left" Margin="10,51,0,0" TextWrapping="Wrap" Text="Home Residence:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeResidence" HorizontalAlignment="Left" Height="16" Margin="106,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockNeighborhood" HorizontalAlignment="Left" Margin="271,51,0,0" TextWrapping="Wrap" Text="Spokane Neighborhood:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeNeighborhood" HorizontalAlignment="Left" Height="16" Margin="406,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockCountyDetail" HorizontalAlignment="Left" Margin="571,51,0,0" TextWrapping="Wrap" Text="County Detail:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCountyDetail" HorizontalAlignment="Left" Height="16" Margin="650,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>

        
        <Button x:Name="buttonBack" Content="Back" Margin="10,30,0,0" VerticalAlignment="Top" BorderThickness="0" Height="16" FontSize="10.667" HorizontalAlignment="Left" Width="25" Click="buttonBack_Click"/>
        <Button x:Name="buttonUpdate" Content="Update" Margin="0,30,10,0" VerticalAlignment="Top" Height="16" BorderThickness="0" FontSize="10.667" Click="buttonUpdate_Click" HorizontalAlignment="Right" Width="37"/>
    </Grid>
</Window>
