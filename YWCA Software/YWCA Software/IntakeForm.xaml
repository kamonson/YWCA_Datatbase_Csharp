        <TextBlock x:Name="textBlockMI" HorizontalAlignment="Left" Margin="190,48,0,0" TextWrapping="Wrap" Text="Middle Initial:" VerticalAlignment="Top"/>
        <TextBox  x:Name="textBoxMI" HorizontalAlignment="Left" Height="16" Margin="267,48,0,0" TextWrapping="Wrap" Text="{Binding Mi}" VerticalAlignment="Top" Width="12" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockLNameLabel" HorizontalAlignment="Left" Margin="306,48,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top"/>
