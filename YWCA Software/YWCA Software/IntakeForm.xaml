<Window x:Class="YWCA_Software.IntakeForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Intake Form" SnapsToDevicePixels="True" ResizeMode="CanResize">

    <Viewbox HorizontalAlignment="Left"  Stretch="Fill">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="125*"/>
                <ColumnDefinition Width="173*"/>
            </Grid.ColumnDefinitions>
            <!--///////////////////////////////////////////////////////////////////////////// Start Demographic Info /////////////////////////////////////////////////////////////////////////////-->

            <Image x:Name="image" HorizontalAlignment="Left" Height="651" VerticalAlignment="Top" Width="894" Source="Resources/advp_snip.png" Stretch="Fill" Grid.ColumnSpan="2" />
            <TextBox x:Name="textBlockPidLabel" HorizontalAlignment="Left" Height="16" Margin="4,91,0,0" TextWrapping="Wrap" Text="Participant ID:" VerticalAlignment="Top" Width="86" BorderThickness="0" IsReadOnly="True" Background="Transparent" Foreground="White"/>
            <TextBlock x:Name="textBlockPid" HorizontalAlignment="Left" Margin="95,91,0,0" TextWrapping="Wrap" Text="{Binding Pid}" Width="70" VerticalAlignment="Top" Height="16" Background="Transparent" Foreground="White"/>
            <TextBox x:Name="textBlockHMISIdLabel" Background="Transparent" HorizontalAlignment="Left" Height="16" Margin="170,91,0,0" TextWrapping="Wrap" Text="HMIS ID:" VerticalAlignment="Top" Width="53" BorderThickness="0" IsReadOnly="True" Foreground="White"/>
            <TextBlock x:Name="textBlockHMISId" HorizontalAlignment="Left" Margin="223,91,0,0" TextWrapping="Wrap" Text="{Binding HmisId}" Width="70" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <TextBox x:Name="textBlockInfoNeLabel" Background="Transparent" HorizontalAlignment="Left" Height="16" Margin="298,91,0,0" TextWrapping="Wrap" Text="Info Net:" VerticalAlignment="Top" Width="61" BorderThickness="0" IsReadOnly="True" Foreground="White"/>
            <TextBlock x:Name="textBlockInfoNet" HorizontalAlignment="Left" Margin="361,91,0,0" TextWrapping="Wrap" Text="{Binding InfoNetId}" Width="70" VerticalAlignment="Top" Height="16" Foreground="White" Grid.ColumnSpan="2"/>
            <TextBlock x:Name="textBlockStaff" HorizontalAlignment="Left" Margin="64,91,0,0" TextWrapping="Wrap" Text="Staff:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxStaff" Height="16" Margin="95,91,342,0" TextWrapping="Wrap" Text="{Binding Staff}" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBlock x:Name="textBlockDateDataEntered" HorizontalAlignment="Left" Margin="182,91,0,0" TextWrapping="Wrap" Text="Date Data Entered:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox  x:Name="textBoxDateDataEntered" HorizontalAlignment="Left" Height="16" Margin="286,91,0,0" TextWrapping="Wrap" Text="{Binding DateDataEntered}" VerticalAlignment="Top" Width="116" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBlock x:Name="textBlockFNameLabel" HorizontalAlignment="Left" Margin="5,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="White"><Run Text="First "/><Run Text="Name:"/></TextBlock>
            <TextBox  x:Name="textBoxFName" HorizontalAlignment="Left" Height="16" Margin="70,122,0,0" TextWrapping="Wrap" Text="{Binding FirstName}" VerticalAlignment="Top" Width="80" BorderThickness="0" GotFocus="GotFocus"/>
            <TextBlock x:Name="textBlockMI" HorizontalAlignment="Left" Margin="185,122,0,0" TextWrapping="Wrap" Text="Middle Initial:" VerticalAlignment="Top" Foreground="White"/>
            <TextBox  x:Name="textBoxMI" HorizontalAlignment="Left" Height="16" Margin="262,122,0,0" TextWrapping="Wrap" Text="{Binding Mi}" VerticalAlignment="Top" Width="12" BorderThickness="0" GotFocus="GotFocus"/>
            <TextBlock x:Name="textBlockLNameLabel" HorizontalAlignment="Left" Margin="301,122,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top" Foreground="White"/>
            <TextBox  x:Name="textBoxLName" HorizontalAlignment="Left" Height="16" Margin="370,122,0,0" TextWrapping="Wrap" Text="{Binding LastName}" VerticalAlignment="Top" Width="145" BorderThickness="0" GotFocus="GotFocus" Grid.ColumnSpan="2"/>
            <TextBlock x:Name="textBlockDOB" HorizontalAlignment="Left" Margin="183,122,0,0" TextWrapping="Wrap" Text="DOB:" VerticalAlignment="Top" Grid.Column="1" Foreground="White"/>
            <TextBox  x:Name="textBoxDOB" HorizontalAlignment="Left" Height="16" Margin="215,122,0,0" TextWrapping="Wrap" Text="{Binding Dob}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <ComboBox  x:Name="comboBoxSSN" HorizontalAlignment="Left" Margin="389,122,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" Text="{Binding Ssn}" Grid.Column="1" >
                <ComboBoxItem Name="Refused">Refused</ComboBoxItem>
                <ComboBoxItem Name="DontKnow">Don't Know</ComboBoxItem>
                <ComboBoxItem Name="Missing">Missing</ComboBoxItem>
            </ComboBox>
            <TextBlock x:Name="textBlockHousingStatus" HorizontalAlignment="Left" Margin="4,160,0,0" TextWrapping="Wrap" Text="Housing Status:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <TextBox x:Name="textBoxHomeHousingStatus" HorizontalAlignment="Left" Height="16" Margin="93,160,0,0" TextWrapping="Wrap" Text="{Binding HousingStatus}" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
            <TextBlock x:Name="textBlockNeighborhood" HorizontalAlignment="Left" Margin="304,160,0,0" TextWrapping="Wrap" Text="Spokane Neighborhood:" VerticalAlignment="Top" Height="16" Grid.ColumnSpan="2" Foreground="White"/>
            <TextBox x:Name="textBoxHomeNeighborhood" HorizontalAlignment="Left" Height="16" Margin="64,160,0,0" TextWrapping="Wrap" Text="{Binding Neighborhood}" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockCountyDetail" HorizontalAlignment="Left" Margin="264,160,0,0" TextWrapping="Wrap" Text="County Detail:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxCountyDetail" HorizontalAlignment="Left" Height="16" Margin="343,160,0,0" TextWrapping="Wrap" Text="{Binding CountyDetail}" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockSSN" HorizontalAlignment="Left" Margin="360,122,0,0" TextWrapping="Wrap" Text="SSN:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBlock x:Name="textBlockStreetAddress" HorizontalAlignment="Left" Margin="7,194,0,0" TextWrapping="Wrap" Text="Street Address:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <ComboBox x:Name="comboBoxStreetAddress" HorizontalAlignment="Left" Margin="96,194,0,0" VerticalAlignment="Top" Width="226" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" Text="{Binding StreetAddress}">
                <ComboBoxItem Name="Homeless">Homeless</ComboBoxItem>
            </ComboBox>
            <TextBlock x:Name="textBlockCity" HorizontalAlignment="Left" Margin="358,194,0,0" TextWrapping="Wrap" Text="City:" VerticalAlignment="Top" Height="16" Grid.ColumnSpan="2" Foreground="White"/>
            <TextBox x:Name="textBoxCity" HorizontalAlignment="Left" Height="16" Margin="11,194,0,0" TextWrapping="Wrap" Text="{Binding City}" VerticalAlignment="Top" Width="113"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockState" HorizontalAlignment="Left" Margin="183,194,0,0" TextWrapping="Wrap" Text="State:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxState" HorizontalAlignment="Left" Height="16" Margin="217,194,0,0" TextWrapping="Wrap" Text="{Binding State}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockZip" HorizontalAlignment="Left" Margin="383,194,0,0" TextWrapping="Wrap" Text="Zip:" VerticalAlignment="Top" Height="16" Grid.Column="1" RenderTransformOrigin="-0.567,0.5" Foreground="White"/>
            <TextBox x:Name="textBoxZip" HorizontalAlignment="Left" Height="16" Margin="407,194,0,0" TextWrapping="Wrap" Text="{Binding Zip}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockHomePhone" HorizontalAlignment="Left" Margin="7,224,0,0" TextWrapping="Wrap" Text="Home Phone:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <TextBox x:Name="textBoxHomePhone" HorizontalAlignment="Left" Height="16" Margin="84,224,0,0" TextWrapping="Wrap" Text="{Binding HomePhone}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
            <TextBlock x:Name="textBlockWork" HorizontalAlignment="Left" Margin="234,224,0,0" TextWrapping="Wrap" Text="Work/Message Phone:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <TextBox x:Name="textBoxWork" HorizontalAlignment="Left" Height="16" Margin="358,224,0,0" TextWrapping="Wrap" Text="{Binding WorkPhone}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0" Grid.ColumnSpan="2"/>
            <TextBlock x:Name="textBlockCallTime" HorizontalAlignment="Left" Margin="161,224,0,0" TextWrapping="Wrap" Text="Call Time:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxCallTime" HorizontalAlignment="Left" Height="16" Margin="217,224,0,0" TextWrapping="Wrap" Text="{Binding CallTime}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <CheckBox x:Name="checkBoxSafeToLeaveMessage" IsChecked="{Binding MsgOk}" Content="Safe To Leave Message" HorizontalAlignment="Left" Margin="365,224,0,0" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBlock x:Name="textBlockGender" HorizontalAlignment="Left" Margin="7,257,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="16"><Run Foreground="White" Text="Gender"/><Run Text=":"/></TextBlock>
            <ComboBox x:Name="comboBoxGender" HorizontalAlignment="Left" Text ="{Binding Gender}"  Margin="55,257,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556">
                <ComboBoxItem x:Name="Female" Content="Female"/>
                <ComboBoxItem x:Name="Male" Content="Male"/>
                <ComboBoxItem x:Name="Transgender" Content="Transgender"/>
                <ComboBoxItem x:Name="Other" Content="Other"/>
            </ComboBox>
            <TextBlock x:Name="textBlockMaritalStatus" HorizontalAlignment="Left" Margin="252,257,0,0" TextWrapping="Wrap" Text="Marital Status:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <ComboBox x:Name="comboBoxMaritalStatus" HorizontalAlignment="Left" Margin="332,257,0,0" Text="{Binding MaritalStatus}" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" Grid.ColumnSpan="2">
                <ComboBoxItem x:Name="Married" Content="Married"/>
                <ComboBoxItem x:Name="Single" Content="Single"/>
                <ComboBoxItem x:Name="Divorced" Content="Divorced"/>
                <ComboBoxItem x:Name="Widow" Content="Widow"></ComboBoxItem>
            </ComboBox>
            <TextBlock x:Name="Ethnicity" HorizontalAlignment="Left" Margin="187,257,0,0" TextWrapping="Wrap" Text="Ethnicity:" VerticalAlignment="Top" Height="16" Grid.Column="1" RenderTransformOrigin="0.736,0.75" Foreground="White"/>
            <ComboBox x:Name="comboBoxEthnicity" HorizontalAlignment="Left" Margin="240,257,0,0" Text="{Binding Ethnicity}" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" Grid.Column="1">
                <ComboBoxItem x:Name="Caucasian" Content="Caucasian"/>
                <ComboBoxItem x:Name="AfricanAmerican" Content="African American"/>
                <ComboBoxItem x:Name="Hispanic" Content="Hispanic/Latino"/>
                <ComboBoxItem x:Name="Asian" Content="Asian/Pacific Islander"/>
                <ComboBoxItem x:Name="Multi" Content="Multi-racial"/>
            </ComboBox>
            <CheckBox x:Name="checkBoxVeteranStatus" IsChecked="{Binding VeteranStatus}" Content="Veteran Status" HorizontalAlignment="Left" Margin="410,257,0,0" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBlock x:Name="textBlockDisability" HorizontalAlignment="Left" Margin="9,294,0,0" TextWrapping="Wrap" Text="Disability:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <ComboBox x:Name="comboBoxDisability" HorizontalAlignment="Left" Margin="65,294,0,0" Text="{Binding Disability}" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556">
                <ComboBoxItem x:Name="Physical" Content="Physical"/>
                <ComboBoxItem x:Name="Mental" Content="Mental"/>
                <ComboBoxItem x:Name="Sensory" Content="Sensory"/>
                <ComboBoxItem x:Name="Multiple" Content="Multiple"/>
                <ComboBoxItem x:Name="OtherDisability" Content="Other"/>
            </ComboBox>
            <TextBlock x:Name="textBlockDisability2" HorizontalAlignment="Left" Margin="208,294,0,0" TextWrapping="Wrap" Text="2nd Disability:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <ComboBox x:Name="comboBoxDisability2" HorizontalAlignment="Left" Margin="287,294,0,0" Text="{Binding SecondDisability}" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556" Grid.ColumnSpan="2">
                <ComboBoxItem x:Name="Physical2" Content="Physical"/>
                <ComboBoxItem x:Name="Mental2" Content="Mental"/>
                <ComboBoxItem x:Name="Sensory2" Content="Sensory"/>
                <ComboBoxItem x:Name="Multiple2" Content="Multiple"/>
                <ComboBoxItem x:Name="OtherDisability2" Content="Other"/>
            </ComboBox>
            <TextBlock x:Name="textBlockIncomeType" HorizontalAlignment="Left" Margin="52,294,0,0" TextWrapping="Wrap" Text="Income Type:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxIncomeType" HorizontalAlignment="Left" Height="16" Margin="127,294,0,0" TextWrapping="Wrap" Text="{Binding IncomeType}" VerticalAlignment="Top" Width="135"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
            <TextBlock x:Name="textBlockTotalMonthlyIncome" HorizontalAlignment="Left" Margin="277,294,0,0" TextWrapping="Wrap" Text="Total Monthly Income: $" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxTotalMonthlyIncome" HorizontalAlignment="Left" Height="16" Margin="409,294,0,0" TextWrapping="Wrap" Text="{Binding TotalMonthlyIncome}" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>


            <Button x:Name="buttonUpdate" Content="" Margin="429,91,0,0"  VerticalAlignment="Top" BorderThickness="0" Height="22" FontSize="10.667" HorizontalAlignment="Left" Width="66" Click="buttonUpdate_Click" Grid.Column="1" Foreground="White">
                <Button.Background>
                    <ImageBrush ImageSource="Resources/update_button.png"/>
                </Button.Background>
            </Button>
            <Button x:Name="buttonBack" Content="" Margin="343,40,0,0" VerticalAlignment="Top" BorderThickness="0" Height="37" FontSize="10.667" HorizontalAlignment="Left" Width="35" Click="buttonBack_Click" Foreground="White" Grid.Column="1">
                <Button.Background>
                    <ImageBrush ImageSource="Resources/back_button-01 (1).png"/>
                </Button.Background>
            </Button>
            <!--///////////////////////////////////////////////////////////////////////////// End Demographic Info /////////////////////////////////////////////////////////////////////////////-->


            <!--///////////////////////////////////////////////////////////////////////////// Start ADVP Info /////////////////////////////////////////////////////////////////////////////-->
            <TextBlock x:Name="textBlockIntakeDate" HorizontalAlignment="Left" Margin="9,342,0,0" TextWrapping="Wrap" Text="IntakeDate:" VerticalAlignment="Top" Height="16" Foreground="White"/>
            <ListBox x:Name="listBoxIntakeDate" Margin="74,342,0,0" ItemsSource="{Binding ListDates, UpdateSourceTrigger=PropertyChanged}" SelectionChanged="ListBox_SelectionChanged" HorizontalAlignment="Left" Width="326" Height="72" VerticalAlignment="Top" Grid.ColumnSpan="2" BorderThickness="0" >
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="{Binding}" />
                        </StackPanel>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
            <TextBlock x:Name="textBlockNewDate" HorizontalAlignment="Left" Margin="30,342,0,0" TextWrapping="Wrap" Text="New Date:" VerticalAlignment="Top" Height="16" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxNewDate" HorizontalAlignment="Left" Height="16" Margin="90,342,0,0" TextWrapping="Wrap" Text="{Binding DateNow}" VerticalAlignment="Top" Width="107" BorderThickness="0" GotFocus="GotFocus" RenderTransformOrigin="1.215,4.062" Grid.Column="1" />
            <Button x:Name="buttonAddNewDate" Content="" Margin="0,339,233,0" VerticalAlignment="Top" Height="21" BorderThickness="0" FontSize="10.667"  HorizontalAlignment="Right" Width="75" Click="buttonAddNewDate_Click" RenderTransformOrigin="0.867,1.238" Grid.Column="1" Foreground="{x:Null}" BorderBrush="{x:Null}">
                <Button.Background>
                    <ImageBrush ImageSource="Resources/newdate_button-01.png"/>
                </Button.Background>
            </Button>
            <TextBlock x:Name="textBlockTotalAdultsInHousehold" HorizontalAlignment="Left" Margin="108,597,0,0" TextWrapping="Wrap" Text="Total Adults In Household:" VerticalAlignment="Top" Grid.Column="1" Foreground="White"/>
            <TextBox x:Name="textBoxTotalAdultsInHousehold" HorizontalAlignment="Left" Height="16" Margin="248,597,0,0" TextWrapping="Wrap" Text="{Binding TotalAdultsInHouseholdString}" VerticalAlignment="Top" Width="37" BorderThickness="0" GotFocus="GotFocus" Grid.Column="1"/>
            <TextBlock x:Name="textBlockTotalChildrenInHousehold" HorizontalAlignment="Left" Margin="288,597,0,0" TextWrapping="Wrap" Text="Total Children In Household:" VerticalAlignment="Top" Grid.Column="1" Foreground="White"/>
