            <ComboBoxItem x:Name="AfricanAmerican" Content="African American"/>
            <ComboBoxItem x:Name="Hispanic" Content="Hispanic/Latino"/>
            <ComboBoxItem x:Name="Asian" Content="Asian/Pacific Islander"/>
            <ComboBoxItem x:Name="Multi" Content="Multi-racial"/>
        </ComboBox>
        <CheckBox x:Name="checkBoxVeteranStatus" IsChecked="{Binding VeteranStatus}" Content="Veteran Status" HorizontalAlignment="Left" Margin="413,134,0,0" VerticalAlignment="Top" Height="16" Grid.Column="1"/>
        <TextBlock x:Name="textBlockDisability" HorizontalAlignment="Left" Margin="10,155,0,0" TextWrapping="Wrap" Text="Disability:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxDisability" HorizontalAlignment="Left" Margin="66,155,0,0" Text="{Binding Disability}" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667" RenderTransformOrigin="-0.113,0.556">
            <ComboBoxItem x:Name="Physical" Content="Physical"/>
            <ComboBoxItem x:Name="Mental" Content="Mental"/>
            <ComboBoxItem x:Name="Sensory" Content="Sensory"/>
            <ComboBoxItem x:Name="Multiple" Content="Multiple"/>
            <ComboBoxItem x:Name="OtherDisability" Content="Other"/>
        </ComboBox>
        <TextBlock x:Name="textBlockDisability2" HorizontalAlignment="Left" Margin="209,155,0,0" TextWrapping="Wrap" Text="2nd Disability:" VerticalAlignment="Top" Height="16"/>
