            <TextBlock x:Name="textBlockHomePhone" HorizontalAlignment="Left" Margin="86,315,0,0" TextWrapping="Wrap" Text="Home Phone:" VerticalAlignment="Top" Height="15" Foreground="White" Width="72"/>
