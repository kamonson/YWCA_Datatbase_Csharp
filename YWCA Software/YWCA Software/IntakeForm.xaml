<Window x:Class="YWCA_Software.IntakeForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YWCA_Software"
        mc:Ignorable="d"
        Title="IntakeForm" Height="900" Width="900" SnapsToDevicePixels="True">
    <Grid>
        <!--<Image x:Name="image" HorizontalAlignment="Left" Height="932" VerticalAlignment="Top" Width="910" Source="Resources/IntakeBKG.png" Stretch="Fill" Margin="-10,-40,-8,0" />-->
        <TextBlock x:Name="textBlockStaff" HorizontalAlignment="Left" Margin="209,10,0,0" TextWrapping="Wrap" Text="Staff:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312"/>
        <TextBox x:Name="textBoxStaff" Height="16" Margin="240,10,572,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockDateDataEntered" HorizontalAlignment="Left" Margin="540,10,0,0" TextWrapping="Wrap" Text="Date Data Entered:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxDateDataEntered" HorizontalAlignment="Left" Height="16" Margin="644,10,0,0" TextWrapping="Wrap" Text="Text Box" VerticalAlignment="Top" Width="107" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBox x:Name="textBlockPidLabel" HorizontalAlignment="Left" Height="16" Margin="10,31,0,0" TextWrapping="Wrap" Text="Participant ID:" VerticalAlignment="Top" Width="86" BorderThickness="0" IsReadOnly="True"/>
        <TextBlock x:Name="textBlockPid" HorizontalAlignment="Left" Margin="96,31,0,0" TextWrapping="Wrap" Text="{Binding Pid}" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBlockHMISIdLabel" HorizontalAlignment="Left" Height="16" Margin="385,31,0,0" TextWrapping="Wrap" Text="HMIS ID:" VerticalAlignment="Top" Width="53" BorderThickness="0" IsReadOnly="True"/>
        <TextBlock x:Name="textBlockHMISId" HorizontalAlignment="Left" Margin="443,31,0,0" TextWrapping="Wrap" Text="Text Box" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBlockInfoNeLabel" HorizontalAlignment="Left" Height="16" Margin="746,31,0,0" TextWrapping="Wrap" Text="Info Net:" VerticalAlignment="Top" Width="61" BorderThickness="0" IsReadOnly="True"/>
        <TextBlock x:Name="textBlockInfoNet" HorizontalAlignment="Left" Margin="812,31,0,0" TextWrapping="Wrap" Text="Text Box" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBlock x:Name="textBlockFNameLabel" HorizontalAlignment="Left" Margin="12,49,0,0" TextWrapping="Wrap" Text="First Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxFName" HorizontalAlignment="Left" Height="16" Margin="77,49,0,0" TextWrapping="Wrap" Text="{Binding FirstName}" VerticalAlignment="Top" Width="80" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockMI" HorizontalAlignment="Left" Margin="173,49,0,0" TextWrapping="Wrap" Text="Middle Initial:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxMI" HorizontalAlignment="Left" Height="16" Margin="250,49,0,0" TextWrapping="Wrap" Text="{Binding Mi}" VerticalAlignment="Top" Width="12" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockLNameLabel" HorizontalAlignment="Left" Margin="293,49,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLName" HorizontalAlignment="Left" Height="16" Margin="356,50,0,0" TextWrapping="Wrap" Text="{Binding LastName}" VerticalAlignment="Top" Width="145" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockDOB" HorizontalAlignment="Left" Margin="540,49,0,0" TextWrapping="Wrap" Text="DOB:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxDOB" HorizontalAlignment="Left" Height="16" Margin="572,49,0,0" TextWrapping="Wrap" Text="{Binding Dob}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus"/>
        <ComboBox x:Name="comboBoxSSN" HorizontalAlignment="Left" Margin="770,49,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem Name="Refused">Refused</ComboBoxItem>
            <ComboBoxItem Name="DontKnow">Don't Know</ComboBoxItem>
            <ComboBoxItem Name="Missing">Missing</ComboBoxItem>
        </ComboBox>
        <TextBlock x:Name="textBlockHomeResidence" HorizontalAlignment="Left" Margin="12,69,0,0" TextWrapping="Wrap" Text="Home Residence:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeResidence" HorizontalAlignment="Left" Height="16" Margin="108,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockNeighborhood" HorizontalAlignment="Left" Margin="308,69,0,0" TextWrapping="Wrap" Text="Spokane Neighborhood:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeNeighborhood" HorizontalAlignment="Left" Height="16" Margin="443,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockCountyDetail" HorizontalAlignment="Left" Margin="644,69,0,0" TextWrapping="Wrap" Text="County Detail:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCountyDetail" HorizontalAlignment="Left" Height="16" Margin="723,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockSSN" HorizontalAlignment="Left" Margin="746,49,0,0" TextWrapping="Wrap" Text="SSN:" VerticalAlignment="Top" Height="16"/>
        <TextBlock x:Name="textBlockStreetAddress" HorizontalAlignment="Left" Margin="12,90,0,0" TextWrapping="Wrap" Text="Street Address:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxStreetAddress" HorizontalAlignment="Left" Margin="96,90,0,0" VerticalAlignment="Top" Width="226" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem Name="Homeless">Homeless</ComboBoxItem>
        </ComboBox>
        <TextBlock x:Name="textBlockCity" HorizontalAlignment="Left" Margin="357,90,0,0" TextWrapping="Wrap" Text="City:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCity" HorizontalAlignment="Left" Height="16" Margin="385,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="113"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockState" HorizontalAlignment="Left" Margin="540,90,0,0" TextWrapping="Wrap" Text="State:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxState" HorizontalAlignment="Left" Height="16" Margin="574,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockZip" HorizontalAlignment="Left" Margin="760,90,0,0" TextWrapping="Wrap" Text="Zip:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxZip" HorizontalAlignment="Left" Height="16" Margin="784,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockHomePhone" HorizontalAlignment="Left" Margin="12,111,0,0" TextWrapping="Wrap" Text="Home Phone:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomePhone" HorizontalAlignment="Left" Height="16" Margin="89,111,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockWork" HorizontalAlignment="Left" Margin="193,111,0,0" TextWrapping="Wrap" Text="Work/Message Phone:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxWork" HorizontalAlignment="Left" Height="16" Margin="317,111,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockCallTime" HorizontalAlignment="Left" Margin="509,111,0,0" TextWrapping="Wrap" Text="Call Time:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCallTime" HorizontalAlignment="Left" Height="16" Margin="565,111,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <CheckBox x:Name="checkBoxSafeToLeaveMessage" Content="Safe To Leave Message" HorizontalAlignment="Left" Margin="741,111,0,0" VerticalAlignment="Top" Height="16"/>
        <TextBlock x:Name="textBlockMaritalStatus" HorizontalAlignment="Left" Margin="12,132,0,0" TextWrapping="Wrap" Text="Marital Status:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxMaritalStatus" HorizontalAlignment="Left" Margin="92,132,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem x:Name="Married" Content="Married"/>
            <ComboBoxItem x:Name="Single" Content="Single"/>
            <ComboBoxItem x:Name="Divorced" Content="Divorced"/>
            <ComboBoxItem x:Name="Widow" Content="Widow"></ComboBoxItem>
        </ComboBox>
        <TextBlock x:Name="Ethnicity" HorizontalAlignment="Left" Margin="390,132,0,0" TextWrapping="Wrap" Text="Ethnicity:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxEthnicity" HorizontalAlignment="Left" Margin="443,132,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem x:Name="Caucasian" Content="Caucasian"/>
            <ComboBoxItem x:Name="AfricanAmerican" Content="African American"/>
            <ComboBoxItem x:Name="Hispanic" Content="Hispanic"/>
            <ComboBoxItem x:Name="Asian" Content="Asian"/>
        </ComboBox>
        <TextBlock x:Name="textBlockVeteranStatus" HorizontalAlignment="Left" Margin="682,132,0,0" TextWrapping="Wrap" Text="Veteran Status:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxVeteranStatus" HorizontalAlignment="Left" Margin="765,132,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem x:Name="None" Content="None"/>
            <ComboBoxItem x:Name="Active" Content="Active"/>
            <ComboBoxItem x:Name="GuardReserve" Content="Guard/Reserve"/>
        </ComboBox>


        <Button x:Name="buttonUpdate" Content="Update" Margin="0,10,10,0" VerticalAlignment="Top" Height="16" BorderThickness="0" FontSize="10.667" Click="buttonUpdate_Click" HorizontalAlignment="Right" Width="37"/>
        <Button x:Name="buttonBack" Content="Back" Margin="10,10,0,0" VerticalAlignment="Top" BorderThickness="0" Height="16" FontSize="10.667" HorizontalAlignment="Left" Width="25" Click="buttonBack_Click"/>



    </Grid>
</Window>
