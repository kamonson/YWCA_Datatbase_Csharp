            <ComboBoxItem x:Name="OtherDisability2" Content="Other"/>
        </ComboBox>
        <TextBlock x:Name="textBlockIncomeType" HorizontalAlignment="Left" Margin="53,155,0,0" TextWrapping="Wrap" Text="Income Type:" VerticalAlignment="Top" Height="16" Grid.Column="1"/>
        <TextBox x:Name="textBoxIncomeType" HorizontalAlignment="Left" Height="16" Margin="128,155,0,0" TextWrapping="Wrap" Text="{Binding IncomeType}" VerticalAlignment="Top" Width="135"  GotFocus="GotFocus" BorderThickness="0" Grid.Column="1"/>
        <TextBlock x:Name="textBlockTotalMonthlyIncome" HorizontalAlignment="Left" Margin="278,155,0,0" TextWrapping="Wrap" Text="Total Monthly Income: $" VerticalAlignment="Top" Height="16" Grid.Column="1"/>
