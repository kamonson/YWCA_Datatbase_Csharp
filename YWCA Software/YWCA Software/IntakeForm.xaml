        </ComboBox>
        <TextBlock x:Name="textBlockCity" HorizontalAlignment="Left" Margin="361,92,0,0" TextWrapping="Wrap" Text="City:" VerticalAlignment="Top" Height="16" Grid.ColumnSpan="2"/>
