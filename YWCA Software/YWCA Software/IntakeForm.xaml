        <TextBlock x:Name="textBlockSSN" HorizontalAlignment="Left" Margin="365,48,0,0" TextWrapping="Wrap" Text="SSN:" VerticalAlignment="Top" Height="16" Grid.Column="1"/>
        <TextBlock x:Name="textBlockStreetAddress" HorizontalAlignment="Left" Margin="10,92,0,0" TextWrapping="Wrap" Text="Street Address:" VerticalAlignment="Top" Height="16"/>
