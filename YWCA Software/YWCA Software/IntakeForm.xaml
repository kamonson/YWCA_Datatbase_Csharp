<Window x:Class="YWCA_Software.IntakeForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YWCA_Software"
        mc:Ignorable="d"
        Title="IntakeForm" Height="900" Width="900">
    <Grid>
        <TextBlock x:Name="textBlockPid" HorizontalAlignment="Left" Margin="101,10,0,0" TextWrapping="Wrap" Text="{Binding Pid}" Width="100" VerticalAlignment="Top"/>
        <TextBox x:Name="textBlockPidLabel" HorizontalAlignment="Left" Height="22" Margin="10,10,0,0" TextWrapping="Wrap" Text="Participant ID:" VerticalAlignment="Top" Width="86" BorderThickness="0"/>
        <TextBlock x:Name="textBlockFNameLabel" HorizontalAlignment="Left" Margin="206,10,0,0" TextWrapping="Wrap" Text="First Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxFName" HorizontalAlignment="Left" Height="16" Margin="271,10,0,0" TextWrapping="Wrap" Text="{Binding FirstName}" VerticalAlignment="Top" Width="120" BorderThickness="0"/>
        <TextBlock x:Name="textBlockFNameLabel_Copy" HorizontalAlignment="Left" Margin="396,10,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxFName_Copy" HorizontalAlignment="Left" Height="16" Margin="460,10,0,0" TextWrapping="Wrap" Text="{Binding LastName}" VerticalAlignment="Top" Width="198" BorderThickness="0"/>
        <TextBlock x:Name="textBlockDOB" HorizontalAlignment="Left" Margin="663,10,0,0" TextWrapping="Wrap" Text="DOB:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="16" Margin="695,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" BorderThickness="0"/>


    </Grid>
</Window>
