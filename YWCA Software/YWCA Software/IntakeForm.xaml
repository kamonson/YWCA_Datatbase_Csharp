        <TextBlock x:Name="textBlockDateDataEntered" HorizontalAlignment="Left" Margin="239,13,0,0" TextWrapping="Wrap" Text="Date Data Entered:" VerticalAlignment="Top" Height="16" Grid.Column="1"/>
