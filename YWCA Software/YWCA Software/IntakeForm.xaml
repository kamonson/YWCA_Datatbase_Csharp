            <TextBlock x:Name="textBlockWork" HorizontalAlignment="Left" Margin="39,330,0,0" TextWrapping="Wrap" Text="Work/Message Phone:" VerticalAlignment="Top" Height="17" Foreground="White" Width="119"/>
