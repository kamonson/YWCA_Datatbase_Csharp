<Window x:Class="YWCA_Software.IntakeForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YWCA_Software"
        mc:Ignorable="d"
        Title="IntakeForm" Height="900" Width="900">
    <Grid>
        <TextBlock x:Name="textBlockStaff" HorizontalAlignment="Left" Margin="209,10,0,0" TextWrapping="Wrap" Text="Staff:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312"/>
        <TextBox x:Name="textBoxStaff" Height="16" Margin="240,10,572,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockDateDataEntered" HorizontalAlignment="Left" Margin="540,10,0,0" TextWrapping="Wrap" Text="Date Data Entered:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxDateDataEntered" HorizontalAlignment="Left" Height="16" Margin="644,10,0,0" TextWrapping="Wrap" Text="Text Box" VerticalAlignment="Top" Width="107" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBox x:Name="textBlockPidLabel" HorizontalAlignment="Left" Height="16" Margin="10,31,0,0" TextWrapping="Wrap" Text="Participant ID:" VerticalAlignment="Top" Width="86" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockPid" HorizontalAlignment="Left" Margin="96,31,0,0" TextWrapping="Wrap" Text="{Binding Pid}" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBlockHMISIdLabel" HorizontalAlignment="Left" Height="16" Margin="377,31,0,0" TextWrapping="Wrap" Text="HMIS ID:" VerticalAlignment="Top" Width="53" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockHMISId" HorizontalAlignment="Left" Margin="435,31,0,0" TextWrapping="Wrap" Text="Text Box" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBlockInfoNeLabel" HorizontalAlignment="Left" Height="16" Margin="746,31,0,0" TextWrapping="Wrap" Text="INFO Net:" VerticalAlignment="Top" Width="61" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockInfoNet" HorizontalAlignment="Left" Margin="812,31,0,0" TextWrapping="Wrap" Text="Text Box" Width="70" VerticalAlignment="Top" Height="16"/>
        <TextBlock x:Name="textBlockFNameLabel" HorizontalAlignment="Left" Margin="12,50,0,0" TextWrapping="Wrap" Text="First Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxFName" HorizontalAlignment="Left" Height="16" Margin="77,50,0,0" TextWrapping="Wrap" Text="{Binding FirstName}" VerticalAlignment="Top" Width="80" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockMI" HorizontalAlignment="Left" Margin="162,49,0,0" TextWrapping="Wrap" Text="Middle Initial:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxMI" HorizontalAlignment="Left" Height="16" Margin="239,49,0,0" TextWrapping="Wrap" Text="{Binding Mi}" VerticalAlignment="Top" Width="12" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockLNameLabel" HorizontalAlignment="Left" Margin="256,49,0,0" TextWrapping="Wrap" Text="Last Name:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLName" HorizontalAlignment="Left" Height="16" Margin="319,50,0,0" TextWrapping="Wrap" Text="{Binding LastName}" VerticalAlignment="Top" Width="145" BorderThickness="0" GotFocus="GotFocus"/>
        <TextBlock x:Name="textBlockDOB" HorizontalAlignment="Left" Margin="469,50,0,0" TextWrapping="Wrap" Text="DOB:" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxDOB" HorizontalAlignment="Left" Height="16" Margin="501,50,0,0" TextWrapping="Wrap" Text="{Binding Dob}" VerticalAlignment="Top" Width="84" BorderThickness="0" GotFocus="GotFocus"/>
        <ComboBox x:Name="comboBoxSSN" HorizontalAlignment="Left" Margin="614,49,0,0" VerticalAlignment="Top" Width="115" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem Name="Refused">Refused</ComboBoxItem>
            <ComboBoxItem Name="DontKnow">Don't Know</ComboBoxItem>
            <ComboBoxItem Name="Missing">Missing</ComboBoxItem>
        </ComboBox>
        <TextBlock x:Name="textBlockHomeResidence" HorizontalAlignment="Left" Margin="12,69,0,0" TextWrapping="Wrap" Text="Home Residence:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeResidence" HorizontalAlignment="Left" Height="16" Margin="108,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockNeighborhood" HorizontalAlignment="Left" Margin="273,69,0,0" TextWrapping="Wrap" Text="Spokane Neighborhood:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxHomeNeighborhood" HorizontalAlignment="Left" Height="16" Margin="408,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockCountyDetail" HorizontalAlignment="Left" Margin="573,69,0,0" TextWrapping="Wrap" Text="County Detail:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCountyDetail" HorizontalAlignment="Left" Height="16" Margin="652,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="160"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockSSN" HorizontalAlignment="Left" Margin="590,49,0,0" TextWrapping="Wrap" Text="SSN:" VerticalAlignment="Top" Height="16"/>
        <TextBlock x:Name="textBlockStreetAddress" HorizontalAlignment="Left" Margin="12,90,0,0" TextWrapping="Wrap" Text="Street Address:" VerticalAlignment="Top" Height="16"/>
        <ComboBox x:Name="comboBoxStreetAddress" HorizontalAlignment="Left" Margin="96,90,0,0" VerticalAlignment="Top" Width="226" BorderThickness="0" Height="18" IsEditable="True" FontSize="10.667">
            <ComboBoxItem Name="Homeless">Homeless</ComboBoxItem>
        </ComboBox>
        <TextBlock x:Name="textBlockCity" HorizontalAlignment="Left" Margin="329,90,0,0" TextWrapping="Wrap" Text="City:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxCity" HorizontalAlignment="Left" Height="16" Margin="357,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="113"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockState" HorizontalAlignment="Left" Margin="483,90,0,0" TextWrapping="Wrap" Text="State:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxState" HorizontalAlignment="Left" Height="16" Margin="517,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>
        <TextBlock x:Name="textBlockZip" HorizontalAlignment="Left" Margin="628,90,0,0" TextWrapping="Wrap" Text="Zip:" VerticalAlignment="Top" Height="16"/>
        <TextBox x:Name="textBoxZip" HorizontalAlignment="Left" Height="16" Margin="652,90,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="99"  GotFocus="GotFocus" BorderThickness="0"/>


        <Button x:Name="buttonUpdate" Content="Update" Margin="0,10,10,0" VerticalAlignment="Top" Height="16" BorderThickness="0" FontSize="10.667" Click="buttonUpdate_Click" HorizontalAlignment="Right" Width="37"/>
        <Button x:Name="buttonBack" Content="Back" Margin="10,10,0,0" VerticalAlignment="Top" BorderThickness="0" Height="16" FontSize="10.667" HorizontalAlignment="Left" Width="25" Click="buttonBack_Click"/>



    </Grid>
</Window>
