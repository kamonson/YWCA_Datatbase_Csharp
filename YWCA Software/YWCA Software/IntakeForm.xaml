        <Button x:Name="buttonUpdate" Content="Update" Margin="464,10,0,0"  VerticalAlignment="Top" BorderThickness="0" Height="21" FontSize="10.667" HorizontalAlignment="Left" Width="45" Click="buttonUpdate_Click" Grid.Column="1" Background="OrangeRed" Foreground="White"/>
        <Button x:Name="buttonBack" Content="Back" Margin="10,10,0,0" VerticalAlignment="Top" BorderThickness="0" Height="21" FontSize="10.667" HorizontalAlignment="Left" Width="45" Click="buttonBack_Click" Background="OrangeRed" Foreground="White"/>
