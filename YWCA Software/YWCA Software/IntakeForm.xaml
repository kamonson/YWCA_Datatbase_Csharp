            <ComboBoxItem x:Name="Male" Content="Male"/>
            <ComboBoxItem x:Name="Transgender" Content="Transgender"/>
            <ComboBoxItem x:Name="Other" Content="Other"/>
        </ComboBox>
        <TextBlock x:Name="textBlockMaritalStatus" HorizontalAlignment="Left" Margin="255,134,0,0" TextWrapping="Wrap" Text="Marital Status:" VerticalAlignment="Top" Height="16"/>
