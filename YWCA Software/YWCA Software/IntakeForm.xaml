            <TextBlock x:Name="textBlockLablePersonsInHome" HorizontalAlignment="Left" Margin="108,377,0,0" TextWrapping="Wrap" TextAlignment="Center" Text="Persons In Home:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312" Grid.Column="1" Width="372" FontSize="16" Foreground="White"/>
            <TextBlock x:Name="textBlockLablePersonInHomeRelationship" HorizontalAlignment="Left" Margin="111,394,0,0" TextWrapping="Wrap" Text="Relationship:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312" Grid.Column="1" Foreground="White"/>
            <TextBlock x:Name="textBlockLablePersonInHomeGender" HorizontalAlignment="Left" Margin="268,394,0,0" TextWrapping="Wrap" Text="Gender:" VerticalAlignment="Top" RenderTransformOrigin="0.038,0.312" Grid.Column="1" Foreground="White"/>
