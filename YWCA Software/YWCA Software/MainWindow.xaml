<Window x:Class="YWCA_Software.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="YWCA Database Select" Height="221" Width="413" ResizeMode="NoResize">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="185*"/>
            <ColumnDefinition Width="146*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="image" Height="192" VerticalAlignment="Top" Source="Resources/YWCANewBKG.png" Stretch="Fill" Grid.ColumnSpan="2" />
        <Button Name="ButtonADVP" BorderThickness="0" Content="ADVP" Click="ButtonADVP_Click" Margin="165,65,165,52" Background="OrangeRed" Foreground="White" Grid.ColumnSpan="2" FontSize="24"/>
        <Button x:Name="button" Content="ECAP 1" Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="ECAP 2" Grid.Column="1" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>
    </Grid>

</Window>
